{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: dev",
      "pathMappings": [
        {
          "url": "http://localhost:8080/src",
          "path": "${workspaceFolder}/src"
        }
      ],
      "suggestPathMappingWizard": false
    }
  ],
  "compounds": []
}
